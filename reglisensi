function CekA() {
    sisalisensi=$(($satubulan - $ipreg))
    if [ $satubulan = 0 ]; then
        clear
        echo -e "
 ${yellow}┌─────────────────────────────────────────────────────────┐${NC}
 ${yellow}│${NC}                  ${GREEN}INFORMASI DATA ADMIN${NC}                   ${yellow}│${NC}
 ${yellow}└─────────────────────────────────────────────────────────┘${NC}
 ${yellow}┌─────────────────────────────────────────────────────────┐${NC}
 ${yellow}│${NC}   ${RED}Lisensi anda tidak cukup untuk melakukan registrasi${NC}   ${yellow}│${NC}
 ${yellow}│${NC}  ${RED}Saat ini anda belum bisa melakukan registrasi lisensi${NC}  ${yellow}│${NC}
 ${yellow}│${NC}   ${RED}silahkan tambah lisensi Admin Kamu terlebih dahulu${NC}    ${yellow}│${NC}
 ${yellow}│${NC}              ${grenbo}SALAM HANGAT ALFVPN COMMUNITY${NC}              ${yellow}│${NC}
 ${yellow}│─────────────────────────────────────────────────────────│${NC}
 ${yellow}│${NC}                    ${yellow}KONTAK REGISTRASI${NC}                    ${yellow}│${NC}
 ${yellow}│${NC}     ${Blue}|Telegram: @Mass_Alfin | WhatsApp: 087749311100|${NC}    ${yellow}│${NC}
 ${yellow}└─────────────────────────────────────────────────────────┘${NC}"
        echo -e " "
        exit 0
    else
        masalisaktif="30"
        RegA
    fi
}

