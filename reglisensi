#!/bin/bash
# COLOR VALIDATION
RED='\033[0;31m'
NC='\033[0m'
gray='\e[1;30m'
Blue='\033[36m'
GREEN='\033[0;32m'
grenbo='\e[92;1m'
yellow='\033[0;33m'
tokgit=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
TIMES="10"
KEY="6486088494:AAHK00b5qJoWHCxJtCaOeYUMXhYNOLThu78"
COMMAND=$(curl -sS https://raw.githubusercontent.com/takbebeh/Tunneling/main/Sandi/cominstall)
URL="https://api.telegram.org/bot$KEY/sendMessage"
MYIP=$(curl -s ipv4.icanhazip.com)
ADMM=$(echo Admin)
MOD="Client"
IZIN=$(curl -sS https://raw.githubusercontent.com/takbebeh/Registrasi/main/ipvip | grep $MYIP | awk '{print $5}')
if [ $IZIN = $ADMM ]; then
    echo -e " "
else
    echo -e "\e[31mPermission Denied!\e[0m";
    exit 0
fi

function RegA() {
    git clone https://github.com/takbebeh/Registrasi.git &> /dev/null
    git config --global user.email "alfinproject22@gmail.com" &> /dev/null
    git config --global user.name "takbebeh" &> /dev/null
    cd /root/Registrasi/
    rm -rf .git
    git init  &> /dev/null
    sed -i "s/### $namaadmin $PassAdminOK $statusadmin $satubulan $duabulan $tigabulan $satutahun $idchat/### $namaadmin $PassAdminOK $statusadmin $sisalisensi $duabulan $tigabulan $satutahun $idchat/g" /root/Registrasi/dataadmin
    echo "### $PassAdminMantap $ipreg $clientreg $masalisaktif $namaadmin $MOD" >> /root/Registrasi/client
    git add .  &> /dev/null
    git commit -m renew &> /dev/null
    git branch -M main &> /dev/null
    git remote add origin https://github.com/takbebeh/Registrasi &> /dev/null
    git push -f https://${tokgit}@github.com/takbebeh/Registrasi.git &> /dev/null
    rm -rf /root/Registrasi
    TEXT="
---------------
Registrasi Lisensi Done
---------------
<code>Client Name:</code> <code>${clientreg}</code>
<code>Password   :</code> <code>${passwordreg}</code>
<code>Masa Aktif :</code> <code>${masalisaktif} Hari</code>
---------------
<code>Order By :</code> <code>${namaadmin}</code>
---------------
OS Support
☞ Debian 10 (recommended)
☞ Debian 11
☞ Ubuntu 20.04 LTS
---------------
Command Instalasi:
<code>$COMMAND</code>
---------------
<b>Thanks To @Mass_Alfin</b>
---------------"
    curl -s --max-time $TIMES -d "chat_id=${idchat}&disable_web_page_preview=1&text=$TEXT&parse_mode=html" $URL >/dev/null
    clear
    echo -e "
 ${yellow}┌─────────────────────────────────────────────────────────┐${NC}
 ${yellow}│${NC}            ${GREEN}INFORMASI REGISTRASI LISENSI [DONE]${NC}          ${yellow}│${NC}
 ${yellow}└─────────────────────────────────────────────────────────┘${NC}
 ${yellow}┌─────────────────────────────────────────────────────────┐${NC}
 ${yellow}│${NC}                 ${GREEN}IDENTITAS LISENSI CLIENT${NC}                ${yellow}│${NC}
 ${yellow}│─────────────────────────────────────────────────────────│${NC}
 ${yellow}│${NC}     ${Blue}Client Name${NC}: ${grenbo}${clientreg}${NC}
 ${yellow}│${NC}     ${Blue}Password   ${NC}: ${grenbo}${passwordreg}${NC}
 ${yellow}│${NC}     ${Blue}Masa Aktif ${NC}: ${grenbo}${masalisaktif} Hari${NC}
 ${yellow}└─────────────────────────────────────────────────────────┘${NC}
 ${yellow}┌─────────────────────────────────────────────────────────┐${NC}
 ${yellow}│${NC}             ${GREEN}IDENTITAS ADMIN LISENSI [UPDATE]${NC}            ${yellow}│${NC}
 ${yellow}│─────────────────────────────────────────────────────────│${NC}
 ${yellow}│${NC}  ${Blue}Admin Name${NC}: ${grenbo}${namaadmin}${NC}   ${Blue}Status Admin${NC}: ${yellow}${statusadmin}${NC}
 ${yellow}│${NC}     ${Blue}1 Bulan${NC}: ${grenbo}${sisalisensi} Lisensi${NC}        ${Blue}3 Bulan${NC}: ${grenbo}${tigabulan} Lisensi${NC}
 ${yellow}│${NC}     ${Blue}2 Bulan${NC}: ${grenbo}${duabulan} Lisensi${NC}        ${Blue}1 Tahun${NC}: ${grenbo}${satutahun} Lisensi${NC}
 ${yellow}└─────────────────────────────────────────────────────────┘${NC}
 ${yellow}Notifikasi Lisensi Akan Dikirimkan Oleh Bot Ke Akun [ADMIN]${NC}
      ${yellow}Silahkan Periksa Chat Telegram Dari @alf_robot${NC}"
    read -n 1 -s -r -p "  Press any key to back on menu"
    menu
}

